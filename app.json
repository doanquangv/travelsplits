{
  "expo": {
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "svgx"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "name": "travelsplits",
    "slug": "travelsplits",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.travelsplitscompany.travelsplits",
      "supportsTablet": true,
      "config": {}
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.travelsplitscompany.travelsplits"
  },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font",
      "expo-asset",
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TravelSplits to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-localization"
    ]
  }
}
