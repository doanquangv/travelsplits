{
  "expo": {
    "packagerOpts": {
      "sourceExts": ["js", "jsx", "svgx"],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "name": "travelsplits",
    "slug": "travelsplits",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "light",
    "permissions": [
      "location"
    ],
    "splash": {
      
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.travelsplitscompany.travelsplits",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCSIz9HM592pvONJ1i-dQuu0_QQ6sEUkSo"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.travelsplitscompany.travelsplits"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset",
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TravelSplits to use your location."
        }
      ]
    ]
  }
}
